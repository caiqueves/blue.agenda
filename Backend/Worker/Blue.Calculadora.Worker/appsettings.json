{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "RabbitMQConfig": {
    "HostName": "localhost", // substitua pelo nome ou endereço IP do seu servidor RabbitMQ
    "Port": 5672, // porta padrão do RabbitMQ
    "UserName": "guest", // substitua pelo nome de usuário do RabbitMQ
    "Password": "guest", // substitua pela senha do RabbitMQ
    "VirtualHost": "/" // substitua pelo Virtual Host, se aplicável (por padrão, geralmente é "/")
  },
  "MySQLConfig": {
    "MYSQL_DBHOST": "127.0.0.1",
    "MYSQL_DBPORT": "3306",
    "MYSQL_PASSWORD": "guest",
    "MYSQL_USER": "guest",
    "MYSQL_DATABASE": "Calculadora"
  }
}